    "type": "library",
    "homepage": "https://github.com/ssnepenthe/soter",
    "license": "GPL-2.0",
    "require": {
        "php": ">=5.4",
        "ssnepenthe/composer-utilities": "^0.1.0",
        "symfony/console": "^3.0"
    },
    "autoload": {
    	"files": ["wp-cli.php"],
        "psr-4": { "SSNepenthe\\Soter\\": "src/" }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:ssnepenthe/composer-utilities.git"
        }
    ],
    "bin": ["bin/soter"]
